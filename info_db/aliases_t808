export dirInfo='/vagrant/dockerStuff/d_sqlite'
alias add='perl -I $dirInfo $dirInfo/add_info.pl'
alias get='perl -I $dirInfo $dirInfo/get_tag.pl'
alias getall='perl -I $dirInfo $dirInfo/get_all.pl'
alias getid='perl -I $dirInfo $dirInfo/get_tag_by_id.pl'
alias del='perl -I $dirInfo $dirInfo/del_id.pl'
alias mod='perl -I $dirInfo $dirInfo/modify_id.pl'
alias modify='perl -I $dirInfo $dirInfo/modify_id.pl'
alias tags='perl -I $dirInfo $dirInfo/get_tags.pl'

